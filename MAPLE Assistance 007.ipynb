   "source": [
    "from math import factorial as fact\n",
    "\n",
    "def choose(n, r):\n",
    "    return fact(n)/(fact(r)*fact(n-r))\n",
    "\n",
    "# choose(3,2)\n",
    "\n",
    "1-fact(20)*choose(365,20)/365**20"
   ]
