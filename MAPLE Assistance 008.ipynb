   "source": [
    "from random import randint\n",
    "\n",
    "group_size = 100\n",
    "N = 100000\n",
    "\n",
    "total = 0\n",
    "for i in range(N):\n",
    "    birthdays = [randint(1,365) for x in range(group_size)]\n",
    "    if len(set(birthdays)) < group_size:\n",
    "        total += 1\n",
    "\n",
    "print(total/N)"
   ]
