   "source": [
    "from math import factorial as fact\n",
    "from math import gamma\n",
    "\n",
    "def choose(n, r):\n",
    "    return fact(n)/(fact(r)*fact(n-r))\n",
    "\n",
    "k = [5, 10, 20, 50, 100]\n",
    "\n",
    "for i in k:\n",
    "    prob = choose(365,i)*i*gamma(i)/365**i\n",
    "    print(f'{i}: {prob}, {1-prob}')"
   ]
